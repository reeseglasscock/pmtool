<div class="ui form add_user_form">
  <div class="field">
    <label>Add Members</label>
    <%= form_for @project do |f| %>
      <%= f.collection_select :user_ids, User.all-@project.users, :id ,:name, { include_blank: false, :include_hidden => false }, { class: "ui dropdown", :multiple => true } %>
      <%= f.submit 'Add Users', class: 'ui button'%>
    <% end %>
    </select>
  </div>
</div>
